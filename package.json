  "name": "single-dog",
  "version": "1.0.0",
  "versionToken": "base_cityfm_hangzhou_single_dog",
  "description": "全国单身踢馆歌手大赛",
  "scripts": {
    "start": "npm run dev:ssr",
    "dev:ssr": "better-npm-run dev:ssr",
    "dev:spa": "better-npm-run dev:spa",
    "pre:ssr": "better-npm-run pre:ssr",
    "pre:spa": "better-npm-run pre:spa",
    "build:ssr": "better-npm-run build:ssr",
    "build:spa": "better-npm-run build:spa",
    "upload": "node config/ftp.js",
    "tinypng": "gulp --gulpfile config/tinypng.js tinypng",
    "upload:map": "node config/mapFileUpload.js"
  },
  "betterScripts": {
    "dev:ssr": {
      "command": "nodemon ./server/server.dev.js --watch server --watch config --watch proxy",
      "env": {
        "NODE_ENV": "development",
        "RENDER_MODE": "ssr"
      }
    },
    "dev:spa": {
      "command": "nodemon ./server/server.dev.js --watch server --watch config --watch proxy",
      "env": {
        "NODE_ENV": "development",
        "RENDER_MODE": "spa"
      }
    },
    "pre:ssr": {
      "command": "webpack --config config/webpack.config.prod.js&&node ./dist/server/server.js",
      "env": {
        "NODE_ENV": "development",
        "RENDER_MODE": "ssr"
      }
    },
    "pre:spa": {
      "command": "webpack --config config/webpack.config.prod.js&&node ./server/server.pre.spa.js",
      "env": {
        "NODE_ENV": "development",
        "RENDER_MODE": "spa"
      }
    },
    "build:ssr": {
      "command": "webpack --config config/webpack.config.prod.js",
      "env": {
        "NODE_ENV": "production",
        "RENDER_MODE": "ssr"
      }
    },
    "build:spa": {
      "command": "webpack --config config/webpack.config.prod.js",
      "env": {
        "NODE_ENV": "production",
        "RENDER_MODE": "spa"
      }
    }
  },
