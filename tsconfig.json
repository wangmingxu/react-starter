{
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "noImplicitAny": false,
    "module": "esnext",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "declaration": false,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "typeRoots": [
        "node_modules/@types"
    ],
    "lib": [
        "es2017",
        "dom"
    ],
  }
}
