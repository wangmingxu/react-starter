{
  "presets": [
    [
      "@babel/preset-env",
      {
        "loose": true,
        "targets": {
          "browsers": [
            "last 2 versions",
            "ios >= 9"
          ],
          "node": "current"
        },
        "modules": false,
        "debug": false,
        "useBuiltIns": "usage",
        "include": [],
        "exclude": []
      }
    ]
  ],
  "plugins": [
    /**转换jsx**/
    "@babel/plugin-transform-react-jsx",
    /**支持class-properties语法*/
    "@babel/plugin-proposal-class-properties",
    /**支持import异步加载语法*/
    "@babel/plugin-syntax-dynamic-import",
    [
      "import", {
        "libraryName": "antd-mobile",
        "style": true,
      },
    ]
  ]
}
